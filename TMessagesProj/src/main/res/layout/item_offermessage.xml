<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <View
        android:id="@+id/background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="1dp"/>

    <org.telegram.ui.Heymate.widget.RoundedCornersImageView
        android:id="@+id/image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/subcategory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/description"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <View
        android:id="@+id/holder_fixed_price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <org.telegram.ui.Components.RadioButton
        android:id="@+id/radio_fixed_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/title_fixed_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/info_fixed_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/price_fixed_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/price_info_fixed_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:elevation="1dp"/>

    <View
        android:id="@+id/holder_bundle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <org.telegram.ui.Components.RadioButton
        android:id="@+id/radio_bundle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/title_bundle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/info_bundle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/price_bundle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/price_info_bundle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:elevation="1dp"/>

    <View
        android:id="@+id/holder_subscription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <org.telegram.ui.Components.RadioButton
        android:id="@+id/radio_subscription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/title_subscription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/info_subscription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/price_subscription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/price_info_subscription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/more_details"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="14sp"
        android:elevation="1dp"/>

    <TextView
        android:id="@+id/book"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="14sp"
        android:elevation="1dp"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:padding="8dp"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/forward"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitCenter"
        android:padding="8dp"/>

</merge>