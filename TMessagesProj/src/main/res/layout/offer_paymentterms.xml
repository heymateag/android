<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="14sp"/>

    <TextView
        android:id="@+id/title_delay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

    <TextView
        android:id="@+id/value_delay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

    <View
        android:id="@+id/divider_delay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/title_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

    <TextView
        android:id="@+id/value_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

    <View
        android:id="@+id/divider_cancel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/title_cancel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

    <TextView
        android:id="@+id/value_cancel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

    <View
        android:id="@+id/divider_cancel2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/title_deposit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

    <TextView
        android:id="@+id/value_deposit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="16sp"/>

</merge>